<resources>

    <!-- Base application theme. -->
    <style name="AlertDialog.AppCompat.Light.p" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.green" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorGreen</item>
        <item name="colorPrimaryDark">@color/colorGreenLight</item>
        <item name="colorAccent">@color/colorRed</item>
    </style>


    <!-- Theme with a hidden toolbar that inherits base application theme. -->
    <style name="AppTheme.NoActionBar" parent="AlertDialog.AppCompat.Light">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <!-- Dart theme with an action bar -->
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <!-- Light theme -->
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>